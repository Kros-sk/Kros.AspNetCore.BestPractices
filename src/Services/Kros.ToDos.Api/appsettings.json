{
  "AppConfig": {
    "Endpoint": "https://kros-demo-appsettings-config.azconfig.io",
    "Settings": [ "Base", "ToDos" ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "AllowedHosts": [
    "*"
  ],

  "ConnectionStrings": {
    "DefaultConnection": "Server=server;Initial Catalog=ToDos;Persist Security Info=False;Integrated Security=True;"
  },

  "KormSettings": {
    "DefaultConnection": {
      "AutoMigrate": true
    }
  },

  "RedisCache": {
    "ConnectionString": "localhost:6379",
    "InstanceName": "local",
    "UseRedis": "false"
  },

  "DistributedCacheEntry": {
    "SlidingExpiration": "0.00:30:00"
  },

  "ApplicationInsights": {
    "InstrumentationKey": ""
  },

  "ApiJwtAuthorization": {
    "JwtSecret": "",
    "RequireHttpsMetadata": true
  },

  "SwaggerDocumentation": {
    "Version": "v1",
    "Title": "ToDo Api",
    "Description": "",
    "Contact": {
      "Name": "Kros",
      "Email": "info@kros.sk",
      "Url": "www.kros.sk"
    }
  }
}